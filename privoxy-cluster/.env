# Configuracion del cluster Privoxy
PROXY_NETWORK=privoxy-cluster-net
BALANCER_PORT=8080
LOG_LEVEL=info

# Numero de instancias hijas
HIJOS_COUNT=4

# Interfaces de red para cada hijo
HIJO1_INTERFACE=wlp0s20f3
HIJO2_INTERFACE=docker0
HIJO3_INTERFACE=docker0
HIJO4_INTERFACE=docker0

# Configuracion de puertos
HIJOS_PORT_BASE=8118

# Configuracion de recursos
MEMORY_LIMIT=512M
CPU_LIMIT=0.5